{
  "name": "@nx-demo-repo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "watchManCacheClean": "watchman watch-del-all",
    "metroCacheClean": " rm -rf $TMPDIR/metro-bundler-cache-* && npm cache clean --force",
    "deleteNodeModules": "rm -rf node_modules && rm -rf package-lock.json && rm -rf apps/webpack-test/ios/Pods && rm -rf apps/webpack-test/ios/Podfile.lock",
    "deleteAndInstall": "yarn deleteNodeModules && yarn watchManCacheClean && yarn npmInstall",
    "npmInstall": "npm i --legacy-peer-deps && rm -rf /tmp/metro-* && yarn launchWeb",
    "web": "npx nx reset && npx nx start test-web-app",
    "launchWeb": "yarn launchChromeWithDev && yarn web",
    "launchChromeWithDev": "open -n -a /Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --args --user-data-dir='/tmp/chrome_dev_test' --disable-web-security --auto-open-devtools-for-tabs",
    "webBuild": "npx nx export test-web-app --platform=web"
  },
  "private": true,
  "dependencies": {
    "@expo/metro-config": "~0.10.7",
    "@expo/webpack-config": "^19.0.0",
    "expo": "49.0.16",
